                    <ITEM NAME="CONTENTTYPE" VALUE="setContentType"/>
                    <ITEM NAME="CACHECONTROL" VALUE="setCacheControl"/>
                    <ITEM NAME="CONTENTLENGTH" VALUE="setContentLength"/>
                    <ITEM NAME="CONTENTENCODING" VALUE="setContentEncoding"/>
                </ITEMS>
            </ITEM>
            <ITEM NAME="META_VALUE" />
          </ITEMS>
        </PARAMETER>    	


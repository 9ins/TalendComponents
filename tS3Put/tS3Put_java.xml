    <PARAMETER
        NAME="ENABLE-SERVER-SIDE-ENCRYPTION"
        FIELD="CHECK"
        NUM_ROW="85"
        REQUIRED="false"
    >
    	<DEFAULT>false</DEFAULT>
    </PARAMETER>
    
    <PARAMETER
        NAME="DIE_ON_ERROR"
        FIELD="CHECK"
        NUM_ROW="90"
    >
        <DEFAULT>false</DEFAULT>
    </PARAMETER>

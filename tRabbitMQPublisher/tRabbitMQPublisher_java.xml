<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT> 
  <HEADER AUTHOR="Kooin-Shin" 
  		  COMPATIBILITY="ALL" 
  		  DATA_AUTO_PROPAGATE="false"
  		  SCHEMA_AUTO_PROPAGATE="true" 
  		  PARALLELIZE="true"
  		  PLATEFORM="ALL" 
  		  RELEASE_DATE="20190906R" 
  		  SERIAL="" 
  		  STARTABLE="true" 
  		  STATUS="RELEASE" 
  		  PARTITIONING="AUTO" 
  		  VERSION="1.00"> 
    <SIGNATURE/> 
  </HEADER>
    
  <FAMILIES> 
    <FAMILY>ChaosToCosmos</FAMILY> 
  </FAMILIES>
    
  <DOCUMENTATION> 
    <URL>http://www.chaostocosmos.org</URL> 
  </DOCUMENTATION>
    
  <CONNECTORS> 
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="1" MAX_OUTPUT="1" /> 
    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="1" MAX_OUTPUT="1" />  
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1" />  
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" MAX_OUTPUT="1" />  
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1" MAX_OUTPUT="1" />  
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="1" MAX_OUTPUT="1 "/>  
    <CONNECTOR CTYPE="RUN_IF" MAX_INPUT="1" MAX_OUTPUT="1" /> 
  </CONNECTORS>
    
  <PARAMETERS>
    <PARAMETER NAME="OUTPUT_SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="1" SHOW="true">    
  	</PARAMETER>
	<PARAMETER NAME="INFO_LABEL_00" FIELD="LABEL" COLOR="55;55;210" GROUP="PROTOCOL_CONFIG" NUM_ROW="2" SHOW="true">
		<DEFAULT></DEFAULT>
	</PARAMETER> 
	<PARAMETER NAME="USE_FIELD_LENGTH_HEADER" FIELD="CHECK" GROUP="PROTOCOL_CONFIG" NUM_ROW="3" SHOW="true">
		<DEFAULT>false</DEFAULT>
	</PARAMETER> 
	<PARAMETER NAME="MESSAGE_CHARSET" FIELD="OPENED_LIST" GROUP="PROTOCOL_CONFIG" NUM_ROW="3" SHOW="true">
		<ITEMS DEFAULT="UTF-8">
			<ITEM NAME="US-ASCII" VALUE="US-ASCII"/>
			<ITEM NAME="UTF-8" VALUE="UTF-8"/>
			<ITEM NAME="UTF-16" VALUE="UTF-16"/>
			<ITEM NAME="UTF-16BE" VALUE="UTF-16BE"/>
			<ITEM NAME="UTF-16LE" VALUE="UTF-16LE"/>
			<ITEM NAME="ISO-8859-1" VALUE="ISO-8859-1"/>
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="FIELD_DELIMITER" FIELD="TEXT" GROUP="PROTOCOL_CONFIG" NUM_ROW="3" SHOW_IF="USE_FIELD_LENGTH_HEADER == 'false'">
		<DEFAULT>"\\u0000"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="10" GROUP="DEFAULT" SHOW="true">
		<DEFAULT>"localhost"</DEFAULT>
	</PARAMETER> 
	<PARAMETER NAME="PORT" FIELD="NUMBER" NUM_ROW="10" GROUP="DEFAULT" SHOW="true">
		<DEFAULT>5672</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="USER_NAME" FIELD="TEXT" NUM_ROW="11" GROUP="DEFAULT" SHOW="true">
		<DEFAULT>"user"</DEFAULT>
	</PARAMETER> 
	<PARAMETER NAME="USER_PASSWORD" FIELD="PASSWORD" NUM_ROW="11" GROUP="DEFAULT" SHOW="true">
		<DEFAULT>"1234"</DEFAULT>
	</PARAMETER>	
	<PARAMETER NAME="EXCHANGE_NAME" FIELD="TEXT" NUM_ROW="12" GROUP="DEFAULT" SHOW="true">
		<DEFAULT>"exchange.name"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="ROUTING_KEY" FIELD="TEXT" NUM_ROW="12" GROUP="DEFAULT" SHOW="true">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	<!-- by Francisco start --> 
	<PARAMETER NAME="VIRTUAL_HOST" FIELD="TEXT" NUM_ROW="13" GROUP="DEFAULT" SHOW="true">
		<DEFAULT>"/"</DEFAULT>
	</PARAMETER> 
	<!-- by Francisco end -->
	
	<PARAMETER NAME="DECLARE_EXCHANGE" FIELD="CHECK" NUM_ROW="50" SHOW="true">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="EXCHANGE_TYPE" FIELD="CLOSED_LIST" NUM_ROW="51" GROUP="EXCHANGE" SHOW_IF="DECLARE_EXCHANGE == 'true'">
		<ITEMS DEFAULT="TOPIC">
			<ITEM NAME="TOPIC" VALUE="topic"/>
			<ITEM NAME="DIRECT" VALUE="direct"/>
			<ITEM NAME="HEADERS" VALUE="headers"/>
			<ITEM NAME="X-JMS-TOPIC" VALUE="x-jms-topic"/>
			<ITEM NAME="FANOUT" VALUE="fanout"/>
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="EXCHANGE_IS_DURABLE" FIELD="CLOSED_LIST" NUM_ROW="51" GROUP="EXCHANGE" SHOW_IF="DECLARE_EXCHANGE == 'true'">
		<ITEMS DEFAULT="true">
			<ITEM NAME="TRUE" VALUE="true"/>
			<ITEM NAME="FALSE" VALUE="false"/>
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="EXCHANGE_IS_AUTO_DELETE" FIELD="CLOSED_LIST" NUM_ROW="51" GROUP="EXCHANGE" SHOW_IF="DECLARE_EXCHANGE == 'true'">
		<ITEMS DEFAULT="false">
			<ITEM NAME="TRUE" VALUE="true"/>
			<ITEM NAME="FALSE" VALUE="false"/>
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="EXCHANGE_ARGUMENTS" FIELD="TABLE" GROUP="EXCHANGE" NUM_ROW="52" NB_LINES="3" SHOW_IF="DECLARE_EXCHANGE == 'true'" >
		<ITEMS>
    	   	<ITEM NAME="ARGUMENT_KEY" FIELD="TEXT" VALUE="" />
        	<ITEM NAME="ARGUMENT_VALUE" FIELD="TEXT" VALUE="&quot;&quot;" />
		</ITEMS>
    </PARAMETER>	 
	
	<PARAMETER NAME="DECLARE_QUEUE" FIELD="CHECK" NUM_ROW="100" SHOW="true">
		<DEFAULT>false</DEFAULT> 
	</PARAMETER>	
	<PARAMETER NAME="QUEUE_NAME" FIELD="TEXT" NUM_ROW="101" GROUP="QUEUE" SHOW_IF="DECLARE_QUEUE == 'true'">
		<DEFAULT>"queue.name"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="QUEUE_EXCHANGE" FIELD="TEXT" NUM_ROW="101" GROUP="QUEUE" SHOW_IF="DECLARE_QUEUE == 'true'">
		<DEFAULT>"exchange.name"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="QUEUE_ROUTING_KEY" FIELD="TEXT" NUM_ROW="101" GROUP="QUEUE" SHOW_IF="DECLARE_QUEUE == 'true'">
		<DEFAULT>""</DEFAULT>
	</PARAMETER> 
	<PARAMETER NAME="IS_QUEUE_EMPTY" FIELD="CHECK" NUM_ROW="102" GROUP="QUEUE" SHOW_IF="DECLARE_QUEUE == 'true'">
		<DEFAULT>true</DEFAULT> 
	</PARAMETER>	
	<PARAMETER NAME="QUEUE_IS_DURABLE" FIELD="CLOSED_LIST" NUM_ROW="102" GROUP="QUEUE" SHOW_IF="DECLARE_QUEUE == 'true'">
		<ITEMS DEFAULT="true">
			<ITEM NAME="TRUE" VALUE="true"/>
			<ITEM NAME="FALSE" VALUE="false"/>
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="QUEUE_IS_AUTO_DELETE" FIELD="CLOSED_LIST" NUM_ROW="102" GROUP="QUEUE" SHOW_IF="DECLARE_QUEUE == 'true'">
		<ITEMS DEFAULT="false">
			<ITEM NAME="TRUE" VALUE="true"/>
			<ITEM NAME="FALSE" VALUE="false"/>
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="QUEUE_IS_EXCLUSIVE" FIELD="CLOSED_LIST" NUM_ROW="102" GROUP="QUEUE" SHOW_IF="DECLARE_QUEUE == 'true'">
		<ITEMS DEFAULT="false">
			<ITEM NAME="TRUE" VALUE="true"/>
			<ITEM NAME="FALSE" VALUE="false"/>
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="QUEUE_ARGUMENTS" FIELD="TABLE" GROUP="QUEUE" NUM_ROW="103" NB_LINES="3" SHOW_IF="DECLARE_QUEUE == 'true'" >
		<ITEMS>
    	   	<ITEM NAME="ARGUMENT_KEY" FIELD="TEXT" VALUE="" />
        	<ITEM NAME="ARGUMENT_VALUE" FIELD="TEXT" VALUE="&quot;&quot;" />
		</ITEMS>
    </PARAMETER>	
  </PARAMETERS>
    
  <ADVANCED_PARAMETERS> 
	<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="1" SHOW="true">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
  </ADVANCED_PARAMETERS>
    
  <CODEGENERATION> 
    <IMPORTS>    
      <IMPORT MODULE="amqp-client-5.6.0.jar" NAME="amqp-client-5.6.0" REQUIRED="true"/> 
      <IMPORT MODULE="slf4j-api-1.7.26.jar" NAME="slf4j-api-1.7.26" REQUIRED="true"/>
      <IMPORT MODULE="slf4j-jdk14-1.6.1.jar" NAME="slf4j-jdk14-1.6.1" REQUIRED="true"/>
    </IMPORTS> 
  </CODEGENERATION>  
  
  <RETURNS> 
    <RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer"/> 
  </RETURNS> 
</COMPONENT>

  </CONNECTORS>
    
  <PARAMETERS> 
	<PARAMETER NAME="INHERIT_AWS_ROLE" FIELD="CHECK" GROUP="AUTH" NUM_ROW="2">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="ACCESS_KEY" FIELD="TEXT" REQUIRED="true" GROUP="AUTH" NUM_ROW="1" SHOW_IF="(INHERIT_AWS_ROLE == 'false')">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>	
	<PARAMETER NAME="SECRET_KEY" FIELD="PASSWORD" REQUIRED="true" GROUP="AUTH" NUM_ROW="1" SHOW_IF="(INHERIT_AWS_ROLE == 'false')">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
    <PARAMETER NAME="REGION" FIELD="OPENED_LIST" RAW="true" NUM_ROW="3" REQUIRED="true">
     	<ITEMS DEFAULT="AP_NORTHEAST_1">
        	<ITEM NAME="DEFAULT" VALUE="DEFAULT" />
         	<ITEM NAME="US_EAST_1" VALUE="&quot;us-east-1&quot;" /> <!-- (N. Virginia) -->
	        <ITEM NAME="US_WEST_2" VALUE="&quot;us-west-2&quot;" /> <!-- (Oregon) -->
	        <ITEM NAME="EU_WEST_1" VALUE="&quot;eu-west-1&quot;" /> <!-- (Ireland) -->
	        <ITEM NAME="AP_NORTHEAST_1" VALUE="&quot;ap-northeast-1&quot;" /> <!-- (Tokyo) -->
	        <ITEM NAME="AP_SOUTHEAST_1" VALUE="&quot;ap-southeast-1&quot;" /> <!-- (Singapore) -->
	        <ITEM NAME="AP_SOUTHEAST_2" VALUE="&quot;ap-southeast-2&quot;" /> <!-- (Sydney) -->
    	</ITEMS> 
    </PARAMETER>
	<PARAMETER NAME="SMS_TYPE" FIELD="CLOSED_LIST" NUM_ROW="4" REQUIRED="true" GROUP="ATTR">  
     	<ITEMS DEFAULT="TRANSACTIONAL">
        	<ITEM NAME="PROMOTIONAL" VALUE="&quot;Promotional&quot;" /> 
         	<ITEM NAME="TRANSACTIONAL" VALUE="&quot;Transactional&quot;" /> 
    	</ITEMS>
	</PARAMETER>	 
	<PARAMETER NAME="MAX_PRICE" FIELD="TEXT" GROUP="ATTR" NUM_ROW="4">	
		<DEFAULT>"0.50"</DEFAULT>
	</PARAMETER>	
	<PARAMETER NAME="SENDER_ID" FIELD="TEXT" GROUP="ATTR" NUM_ROW="4">	
		<DEFAULT>"SENDER"</DEFAULT>
	</PARAMETER>	 
	<PARAMETER NAME="CREATE_TOPIC" FIELD="CHECK" GROUP="TOPIC" REQUIRED="true" NUM_ROW="5">	
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="TOPIC_NAME" FIELD="TEXT" GROUP="TOPIC" NUM_ROW="5" SHOW_IF="(CREATE_TOPIC == 'true')">	
		<DEFAULT>""</DEFAULT>
	</PARAMETER>	
	<PARAMETER NAME="USE_TOPIC_ARN" FIELD="CHECK" GROUP="TOPIC" REQUIRED="true" NUM_ROW="6">	
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="TOPIC_ARN" FIELD="TEXT" GROUP="TOPIC" REQUIRED="true" NUM_ROW="6" SHOW_IF="(USE_TOPIC_ARN == 'true')">	
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="SUBSCRIBE_TOPIC" FIELD="CHECK" GROUP="TOPIC" REQUIRED="true" NUM_ROW="6"  SHOW_IF="(USE_TOPIC_ARN == 'true')">	
		<DEFAULT>false</DEFAULT>
	</PARAMETER>	
	<PARAMETER NAME="END_POINTS" FIELD="TEXT" NUM_ROW="30" SHOW_IF="((USE_TOPIC_ARN == 'true') and (SUBSCRIBE_TOPIC == 'true')) or (USE_TOPIC_ARN == 'false')">	
		<DEFAULT>""</DEFAULT>
	</PARAMETER>	
	<PARAMETER NAME="END_POINTS_LABEL" FIELD="LABEL" NUM_ROW="31"/>	
	<PARAMETER NAME="MESSAGE" FIELD="MEMO_MESSAGE" REQUIRED="true" NUM_ROW="32">	
		<DEFAULT>""</DEFAULT>
	</PARAMETER>	
	<PARAMETER NAME="PRINT_ARN" FIELD="CHECK" REQUIRED="true" NUM_ROW="33">	
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="VERSION" FIELD="LABEL" REQUIRED="true" NUM_ROW="100">	
		<DEFAULT>v1.01_20200601</DEFAULT>
	</PARAMETER>	
  </PARAMETERS>
   
  <ADVANCED_PARAMETERS> 
  	<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="1" >
		<DEFAULT>false</DEFAULT>
	</PARAMETER>  
  </ADVANCED_PARAMETERS>
    
  <CODEGENERATION> 
    <IMPORTS> 
      <IMPORT MODULE="aws-sns-0.9.1.jar" NAME="aws-sns-0.9.1.jar" REQUIRED="true"/>
      <IMPORT MODULE="aws-java-sdk-1.11.358.jar" NAME="aws-java-sdk-1.11.358" REQUIRED="true"/>  
      <IMPORT MODULE="commons-logging-1.2.jar" NAME="commons-logging-1.2" REQUIRED="true"/>  
      <IMPORT MODULE="httpclient-4.5.5.jar" NAME="httpclient-4.5.5" REQUIRED="true"/>  
      <IMPORT MODULE="httpcore-4.4.9.jar" NAME="httpcore-4.4.9" REQUIRED="true"/>  

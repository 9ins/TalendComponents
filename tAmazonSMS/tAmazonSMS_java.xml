      <IMPORT MODULE="jackson-databind-2.6.7.1.jar" NAME="jackson-databind-2.6.7.1" REQUIRED="true"/>  
      <IMPORT MODULE="joda-time-2.8.1.jar" NAME="joda-time-2.8.1" REQUIRED="true"/> 
    </IMPORTS> 
  </CODEGENERATION>  
  

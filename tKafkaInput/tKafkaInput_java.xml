<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.101" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070210A"
		STARTABLE="true">
		<SIGNATURE />
	</HEADER>

	<FAMILIES>
		<FAMILY>Internet/Kafka</FAMILY>
	</FAMILIES>

	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>

	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>

	<PARAMETERS>
		<!-- PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" SHOW="true" NUM_ROW="1"/ -->

		<PARAMETER NAME="OUTPUT_TYPE" FIELD="CLOSED_LIST" SHOW="true"
			NUM_ROW="5">
			<ITEMS DEFAULT="STRING">
				<ITEM NAME="STRING" VALUE="STRING" />
				<ITEM NAME="BYTES" VALUE="BYTES" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="5"
			REQUIRED="true">
			<TABLE IF="(OUTPUT_TYPE == 'BYTES') AND (SAVE_OFFSET == 'true')"
				READONLY="true">
				<COLUMN NAME="payload" TYPE="id_byte[]" READONLY="true"
					CUSTOM="true" />
				<COLUMN NAME="offset" TYPE="id_Long" READONLY="true" CUSTOM="true" />
			</TABLE>
			<TABLE IF="(OUTPUT_TYPE == 'BYTES') AND (SAVE_OFFSET == 'false')"
				READONLY="true">
				<COLUMN NAME="payload" TYPE="id_byte[]" READONLY="true"
					CUSTOM="true" />
			</TABLE>
			<TABLE IF="(OUTPUT_TYPE == 'STRING') AND (SAVE_OFFSET == 'true')"
				READONLY="true">
				<COLUMN NAME="payload" TYPE="id_String" READONLY="true"
					CUSTOM="true" />
				<COLUMN NAME="offset" TYPE="id_Long" READONLY="true" CUSTOM="true" />
			</TABLE>
			<TABLE IF="(OUTPUT_TYPE == 'STRING') AND (SAVE_OFFSET == 'false')"
				READONLY="true">
				<COLUMN NAME="payload" TYPE="id_String" READONLY="true"
					CUSTOM="true" />
			</TABLE>
		</PARAMETER>

		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST"
			REQUIRED="true" FILTER="tKafkaConnection" NUM_ROW="10"
			SHOW_IF="USE_EXISTING_CONNECTION == 'true'" />

		<PARAMETER NAME="KAFKA_VERSION" FIELD="CLOSED_LIST" NUM_ROW="20"
			REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<ITEMS DEFAULT="KAFKA_1_1_0">
				<ITEM NAME="KAFKA_1_1_0" VALUE="KAFKA_1_1_0" />
				<ITEM NAME="KAFKA_0_10_0_1" VALUE="KAFKA_0_10_0_1" />
				<ITEM NAME="KAFKA_0_9_0_1" VALUE="KAFKA_0_9_0_1" />
				<ITEM NAME="KAFKA_0_8_2_0" VALUE="KAFKA_0_8_2_0" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="ZOOKEEPER_CONNECT" FIELD="TEXT" NUM_ROW="30"
			REQUIRED="false"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false' AND KAFKA_VERSION == 'KAFKA_0_8_2_0')">
			<DEFAULT>"zk1:port1,zk2:port2,..."</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BROKER_LIST" FIELD="TEXT" NUM_ROW="31"
			REQUIRED="false"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false' AND KAFKA_VERSION != 'KAFKA_0_8_2_0')">
			<DEFAULT>"host1:port1,host2:port2,..."</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="KAFKA_TOPIC" FIELD="TEXT" REQUIRED="true"
			NUM_ROW="40" NB_LINES="3">
			<DEFAULT>"mytopic"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="GROUP_ID" FIELD="TEXT" NUM_ROW="50">
			<DEFAULT>"mygroup"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="RESET_OFFSET" FIELD="CHECK" NUM_ROW="60"
			GROUP="OFFSET_MANAGEMENT_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="AUTO_OFFSET_RESET" FIELD="CLOSED_LIST"
			NUM_ROW="70" GROUP="OFFSET_MANAGEMENT_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false' AND KAFKA_VERSION == 'KAFKA_0_8_2_0') 
			         OR (USE_EXISTING_CONNECTION == 'true' AND (#LINK@NODE.CONNECTION.KAFKA_VERSION == 'KAFKA_0_8_2_0'))">
			<ITEMS DEFAULT="LARGEST">
				<ITEM NAME="LARGEST" VALUE="LARGEST" />
				<ITEM NAME="SMALLEST" VALUE="SMALLEST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="AUTO_OFFSET_RESET_NEW" FIELD="CLOSED_LIST"
			NUM_ROW="71" GROUP="OFFSET_MANAGEMENT_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false' AND KAFKA_VERSION != 'KAFKA_0_8_2_0') 
                     OR (USE_EXISTING_CONNECTION == 'true' AND (#LINK@NODE.CONNECTION.KAFKA_VERSION != 'KAFKA_0_8_2_0'))">
			<ITEMS DEFAULT="LATEST">
				<ITEM NAME="LATEST" VALUE="LATEST" />
				<ITEM NAME="EARLIEST" VALUE="EARLIEST" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="KAFKA_OFFSET_STORAGE" FIELD="CLOSED_LIST"
			NUM_ROW="80" GROUP="OFFSET_MANAGEMENT_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false' AND KAFKA_VERSION == 'KAFKA_0_8_2_0') 
                     OR (USE_EXISTING_CONNECTION == 'true' AND (#LINK@NODE.CONNECTION.KAFKA_VERSION == 'KAFKA_0_8_2_0'))">
			<ITEMS DEFAULT="ZOOKEEPER">
				<ITEM NAME="ZOOKEEPER" VALUE="ZOOKEEPER" />
				<ITEM NAME="KAFKA" VALUE="KAFKA" />
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="KAFKA_DUAL_COMMIT_CHECK" FIELD="CHECK"
			NUM_ROW="80"
			SHOW_IF="(KAFKA_OFFSET_STORAGE == 'KAFKA') 
			         AND 
			         ((USE_EXISTING_CONNECTION == 'false' AND KAFKA_VERSION == 'KAFKA_0_8_2_0') 
                        OR (USE_EXISTING_CONNECTION == 'true' AND (#LINK@NODE.CONNECTION.KAFKA_VERSION == 'KAFKA_0_8_2_0')))"
			GROUP="OFFSET_MANAGEMENT_GROUP">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="AUTO_COMMIT_OFFSET" FIELD="CHECK" NUM_ROW="90"
			GROUP="OFFSET_MANAGEMENT_GROUP">
			<DEFAULT>true</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="KAFKA_COMMIT_INTERVAL" FIELD="TEXT"
			NUM_ROW="90" REQUIRED="true" SHOW_IF="(AUTO_COMMIT_OFFSET == 'true')"
			GROUP="OFFSET_MANAGEMENT_GROUP">
			<DEFAULT>5000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_BATCH_MAX_DURATION" FIELD="CHECK"
			NUM_ROW="100" GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BATCH_MAX_DURATION" FIELD="TEXT" NUM_ROW="100"
			REQUIRED="true" SHOW_IF="(USE_BATCH_MAX_DURATION == 'true')" GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>600000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_BATCH_MAX_SIZE" FIELD="CHECK" NUM_ROW="110"
			GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BATCH_MAX_SIZE" FIELD="TEXT" NUM_ROW="110"
			REQUIRED="true" SHOW_IF="(USE_BATCH_MAX_SIZE == 'true')" GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>5000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_BATCH_MESSAGE_TIMEOUT" FIELD="CHECK"
			NUM_ROW="120" GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="BATCH_MESSAGE_TIMEOUT" FIELD="TEXT"
			NUM_ROW="120" REQUIRED="true" SHOW_IF="(USE_BATCH_MESSAGE_TIMEOUT == 'true')"
			GROUP="BATCH_MANAGEMENT_GROUP">
			<DEFAULT>10000</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="USE_HTTPS" FIELD="CHECK" NUM_ROW="130"
			GROUP="SECURITY_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (KAFKA_VERSION != 'KAFKA_0_8_2_0')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="HTTPS_SETTING" FIELD="COMPONENT_LIST"
			FILTER="tSetKeystore" NUM_ROW="130" GROUP="SECURITY_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_HTTPS == 'true') AND (KAFKA_VERSION != 'KAFKA_0_8_2_0')"
			DYNAMIC_SETTINGS="true" />

		<PARAMETER NAME="USE_KRB" FIELD="CHECK" NUM_ROW="140"
			GROUP="SECURITY_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (KAFKA_VERSION != 'KAFKA_0_8_2_0')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="JAAS_CONF" FIELD="FILE" NUM_ROW="141"
			GROUP="SECURITY_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_KRB == 'true') AND (KAFKA_VERSION != 'KAFKA_0_8_2_0')">
			<DEFAULT>"/etc/kafka/kafka_client_jaas.conf"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="KRB_SERVICE_NAME" FIELD="TEXT" NUM_ROW="142"
			GROUP="SECURITY_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_KRB == 'true') AND (KAFKA_VERSION != 'KAFKA_0_8_2_0')">
			<DEFAULT>"kafka"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SET_KINIT_PATH" FIELD="CHECK" NUM_ROW="143"
			GROUP="SECURITY_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_KRB == 'true') AND (KAFKA_VERSION != 'KAFKA_0_8_2_0')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="KINIT_PATH" FIELD="FILE" NUM_ROW="143"
			GROUP="SECURITY_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_KRB == 'true') AND (KAFKA_VERSION != 'KAFKA_0_8_2_0') AND (SET_KINIT_PATH == 'true')">
			<DEFAULT>"/usr/bin/kinit"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SET_KRB5_CONF" FIELD="CHECK" NUM_ROW="144"
			GROUP="SECURITY_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_KRB == 'true') AND (KAFKA_VERSION != 'KAFKA_0_8_2_0')">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="KRB5_CONF" FIELD="FILE" NUM_ROW="144"
			GROUP="SECURITY_GROUP"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') AND (USE_KRB == 'true') AND (KAFKA_VERSION != 'KAFKA_0_8_2_0') AND (SET_KRB5_CONF == 'true')">
			<DEFAULT>"/etc/kafka/krb5.conf"</DEFAULT>
		</PARAMETER>

	</PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="KAFKA_CONSUMER_PROPERTIES" FIELD="TABLE"
			REQUIRED="false" NUM_ROW="40" NB_LINES="3">
			<ITEMS>
				<ITEM NAME="PROPERTY" />
				<ITEM NAME="VALUE" />
			</ITEMS>
		</PARAMETER>

		<!--SHOW_IF="(USE_BATCH_MAX_DURATION == 'true')" -->
		<PARAMETER NAME="KAFKA_CONSUMER_TIMEOUT" FIELD="TEXT"
			NUM_ROW="42" REQUIRED="true">
			<DEFAULT>-1</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="SAVE_OFFSET" FIELD="CHECK" NUM_ROW="45"
			REQUIRED="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="CUSTOM_ENCODING" FIELD="CHECK" NUM_ROW="50"
			REQUIRED="true">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="51"
			REQUIRED="true" SHOW_IF="CUSTOM_ENCODING == 'true'">
			<DEFAULT>"UTF-8"</DEFAULT>
		</PARAMETER>
	</ADVANCED_PARAMETERS>

	<CODEGENERATION>
		<IMPORTS>
			<!-- Kafka libraries for 2.10-0.8.2.0 -->
			<IMPORT MODULE_GROUP="KAFKA-LIB-2_10-0_8_2_0"
				REQUIRED_IF="(KAFKA_VERSION=='KAFKA_0_8_2_0') AND (USE_EXISTING_CONNECTION=='false')" />

			<!-- Kafka libraries for 2.10-0.9.0.1 -->
			<IMPORT MODULE_GROUP="KAFKA-LIB-2_10-0_9_0_1"
				REQUIRED_IF="(KAFKA_VERSION=='KAFKA_0_9_0_1') AND (USE_EXISTING_CONNECTION=='false')" />

			<!-- Kafka libraries for 2.10-0.10.0.1 -->
			<IMPORT MODULE_GROUP="KAFKA-LIB-2_10-0_10_0_1"
				REQUIRED_IF="(KAFKA_VERSION=='KAFKA_0_10_0_1') AND (USE_EXISTING_CONNECTION=='false')" />
				
			<!-- Kafka libraries for 2.11-1.1.0 -->
			<IMPORT MODULE_GROUP="KAFKA-LIB-2_11-1_1_0"
				REQUIRED_IF="(KAFKA_VERSION=='KAFKA_1_1_0') AND (USE_EXISTING_CONNECTION=='false')" />
		</IMPORTS>
	</CODEGENERATION>
	<RETURNS />
</COMPONENT>

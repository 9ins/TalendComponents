<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT> 
  <HEADER AUTHOR="Kooin-Shin" 
  		  COMPATIBILITY="ALL" 
  		  DATA_AUTO_PROPAGATE="false"
  		  SCHEMA_AUTO_PROPAGATE="true" 
  		  PLATEFORM="ALL" 
  		  RELEASE_DATE="20191111R" 
  		  SERIAL="" 
  		  STARTABLE="true" 
  		  STATUS="RELEASE" 
  		  PARTITIONING="AUTO" 
  		  VERSION="1.02"> 
    <SIGNATURE/> 
  </HEADER>
    
  <FAMILIES> 
    <FAMILY>ChaosToCosmos</FAMILY> 
  </FAMILIES>
    
  <DOCUMENTATION> 
    <URL>http://www.chaostocosmos.org</URL> 
  </DOCUMENTATION>
    
  <CONNECTORS> 
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="93" /> 
    <CONNECTOR CTYPE="ITERATE" MAX_INPUT="0" MAX_OUTPUT="93" />  
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" MAX_OUTPUT="93" />  
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" MAX_OUTPUT="93" />  
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1" MAX_OUTPUT="93" />  
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="1" MAX_OUTPUT="93 "/>  
    <CONNECTOR CTYPE="RUN_IF" MAX_INPUT="1" MAX_OUTPUT="93" /> 
  </CONNECTORS>
    
  <PARAMETERS>
    <PARAMETER NAME="OUTPUT_SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="1" SHOW="true">
  	</PARAMETER>
	<PARAMETER NAME="INFO_LABEL_00" FIELD="LABEL" COLOR="55;55;210" GROUP="PROTOCOL_CONFIG" NUM_ROW="2" SHOW="true">
		<DEFAULT></DEFAULT>
	</PARAMETER> 
	<PARAMETER NAME="USE_FIELD_LENGTH_HEADER" FIELD="CHECK" GROUP="PROTOCOL_CONFIG" NUM_ROW="3" SHOW="true">
		<DEFAULT>false</DEFAULT>
	</PARAMETER> 
	<PARAMETER NAME="MESSAGE_CHARSET" FIELD="OPENED_LIST" GROUP="PROTOCOL_CONFIG" NUM_ROW="3" SHOW="true">
		<ITEMS DEFAULT="UTF-8">
			<ITEM NAME="US-ASCII" VALUE="US-ASCII"/>
			<ITEM NAME="UTF-8" VALUE="UTF-8"/>
			<ITEM NAME="UTF-16" VALUE="UTF-16"/>
			<ITEM NAME="UTF-16BE" VALUE="UTF-16BE"/>
			<ITEM NAME="UTF-16LE" VALUE="UTF-16LE"/>
			<ITEM NAME="ISO-8859-1" VALUE="ISO-8859-1"/>
		</ITEMS>
	</PARAMETER>	
	<PARAMETER NAME="FIELD_DELIMITER" FIELD="TEXT" NUM_ROW="3" GROUP="PROTOCOL_CONFIG" SHOW_IF="USE_FIELD_LENGTH_HEADER == 'false'">
		<DEFAULT>"\\u0000"</DEFAULT>
	</PARAMETER>	
	<PARAMETER NAME="VIRTUAL_HOST" FIELD="TEXT" NUM_ROW="10" GROUP="QUEUE" SHOW="true">
		<DEFAULT>"/"</DEFAULT>
	</PARAMETER> 
	<PARAMETER NAME="QUEUE_HOST" FIELD="TEXT" NUM_ROW="11" GROUP="QUEUE" SHOW="true">
		<DEFAULT>"localhost"</DEFAULT>
	</PARAMETER> 
	<PARAMETER NAME="QUEUE_PORT" FIELD="TEXT" NUM_ROW="11" GROUP="QUEUE" SHOW="true">
		<DEFAULT>"5672"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="USER_NAME" FIELD="TEXT" NUM_ROW="12" GROUP="QUEUE" SHOW="true">
		<DEFAULT>"user"</DEFAULT>
	</PARAMETER> 
	<PARAMETER NAME="USER_PASSWORD" FIELD="PASSWORD" NUM_ROW="12" GROUP="QUEUE" SHOW="true">
		<DEFAULT>"1234"</DEFAULT>
	</PARAMETER>	
	<PARAMETER NAME="QUEUE_NAME" FIELD="TEXT" NUM_ROW="13" GROUP="QUEUE" SHOW="true">
		<DEFAULT>"queue.name"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="AUTO_ACK" FIELD="CHECK" NUM_ROW="14" GROUP="QUEUE" SHOW="true">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="USE_CONSUME_DELAY" FIELD="CHECK" NUM_ROW="20" SHOW="true">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="CONSUME_DELAY" FIELD="NUMBER" NUM_ROW="21" SHOW_IF="(USE_CONSUME_DELAY == 'true')">
		<DEFAULT>100</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="DIE_NO_MESSAGE" FIELD="CHECK" NUM_ROW="50"  SHOW="true">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="WAITING_FOR_DIE" FIELD="NUMBER" NUM_ROW="50" SHOW_IF="(DIE_NO_MESSAGE == 'true')">
		<DEFAULT>3</DEFAULT>
	</PARAMETER>	
  </PARAMETERS>
    
  <ADVANCED_PARAMETERS> 
	<PARAMETER NAME="DIE_ON_ERROR" FIELD="CHECK" NUM_ROW="1" SHOW="true">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
  </ADVANCED_PARAMETERS>
    
  <CODEGENERATION> 
    <IMPORTS>    
      <IMPORT MODULE="amqp-client-5.6.0.jar" NAME="amqp-client-5.6.0" REQUIRED="true"/> 
      <IMPORT MODULE="slf4j-api-1.7.26.jar" NAME="slf4j-api-1.7.26" REQUIRED="true"/>
      <IMPORT MODULE="slf4j-jdk14-1.6.1.jar" NAME="slf4j-jdk14-1.6.1" REQUIRED="true"/>
    </IMPORTS> 
  </CODEGENERATION>  
  
  <RETURNS> 
    <RETURN AVAILABILITY="AFTER" NAME="NB_LINE" TYPE="id_Integer"/> 
  </RETURNS> 
</COMPONENT>

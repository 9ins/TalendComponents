<COMPONENT>
	<HEADER PLATEFORM="ALL" SERIAL="" VERSION="0.102" STATUS="ALPHA"
		COMPATIBILITY="ALL" AUTHOR="Talend" RELEASE_DATE="20070314A"
		STARTABLE="true" LOG4J_ENABLED="true">
		<SIGNATURE />
	</HEADER>

  <FAMILIES>
    <FAMILY>Databases/Amazon/Oracle</FAMILY>
    <FAMILY>Cloud/Amazon/Oracle</FAMILY>
  </FAMILIES>
	<DOCUMENTATION>
		<URL />
	</DOCUMENTATION>
	<CONNECTORS>
		<CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="1" />
		<CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="1" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
		<CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
		<CONNECTOR CTYPE="COMPONENT_OK" />
		<CONNECTOR CTYPE="COMPONENT_ERROR" />
		<CONNECTOR CTYPE="RUN_IF" />
	</CONNECTORS>
	<PARAMETERS>
		<PARAMETER NAME="PROPERTY" FIELD="PROPERTY_TYPE" NUM_ROW="1"
			REPOSITORY_VALUE="DATABASE:ORACLE" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" />
		<PARAMETER NAME="USE_EXISTING_CONNECTION" FIELD="CHECK"
			NUM_ROW="2">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CONNECTION" FIELD="COMPONENT_LIST" FILTER="tAmazonOracleConnection" REQUIRED="true"
			NUM_ROW="2" SHOW_IF="USE_EXISTING_CONNECTION == 'true'" DYNAMIC_SETTINGS="true"/>

		<PARAMETER NAME="CONNECTION_TYPE" FIELD="CLOSED_LIST"
			NUM_ROW="3" REPOSITORY_VALUE="CONNECTION_TYPE" SHOW_IF="USE_EXISTING_CONNECTION == 'false'">
			<ITEMS DEFAULT="ORACLE_SID">
				<ITEM NAME="ORACLE_SID" VALUE="ORACLE_SID" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST" NUM_ROW="30"
			REPOSITORY_VALUE="DB_VERSION" SHOW_IF="(USE_EXISTING_CONNECTION == 'false')">
			<ITEMS DEFAULT="ORACLE_11-6">
				<ITEM NAME="ORACLE_12" VALUE="ORACLE_12" />
				<ITEM NAME="ORACLE_11-6" VALUE="ORACLE_11-6" />
				<ITEM NAME="ORACLE_11" VALUE="ORACLE_11" />
			</ITEMS>
		</PARAMETER>
		<PARAMETER NAME="HOST" FIELD="TEXT" NUM_ROW="40"
			REPOSITORY_VALUE="SERVER_NAME"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false' and CONNECTION_TYPE != 'ORACLE_OCI') and (CONNECTION_TYPE != 'ORACLE_WALLET')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TYPE" FIELD="TEXT" NUM_ROW="40" SHOW="false"
			REPOSITORY_VALUE="TYPE">
			<DEFAULT>Oracle</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PORT" FIELD="TEXT" NUM_ROW="40"
			REPOSITORY_VALUE="PORT"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false' and CONNECTION_TYPE != 'ORACLE_OCI') and (CONNECTION_TYPE != 'ORACLE_WALLET')">
			<DEFAULT>"1521"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="DBNAME" FIELD="TEXT" NUM_ROW="50"
			REPOSITORY_VALUE="SID" REQUIRED="true"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false' and CONNECTION_TYPE != 'ORACLE_OCI') and (CONNECTION_TYPE != 'ORACLE_WALLET')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="LOCAL_SERVICE_NAME" FIELD="TEXT" NUM_ROW="50"
			REPOSITORY_VALUE="SID" REQUIRED="true"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false' and CONNECTION_TYPE == 'ORACLE_OCI') and (CONNECTION_TYPE != 'ORACLE_WALLET')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="SCHEMA_DB" FIELD="TEXT" NUM_ROW="50"
			REPOSITORY_VALUE="SCHEMA" SHOW_IF="((USE_EXISTING_CONNECTION == 'false') )">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USER" FIELD="TEXT" NUM_ROW="60"
			REPOSITORY_VALUE="USERNAME" REQUIRED="true"
			SHOW_IF="((USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_WALLET'))">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="PASS" FIELD="PASSWORD" NUM_ROW="60"
			REPOSITORY_VALUE="PASSWORD" REQUIRED="true"
			SHOW_IF="((USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_WALLET'))">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="JDBC_URL" FIELD="TEXT" NUM_ROW="65" REQUIRED="true"
	  		SHOW_IF="(USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE == 'ORACLE_WALLET')">
		<DEFAULT>"jdbc:oracle:oci:/@talend"</DEFAULT>
	 	</PARAMETER>
		<PARAMETER NAME="SCHEMA" FIELD="SCHEMA_TYPE" NUM_ROW="70">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TABLE" FIELD="DBTABLE" NUM_ROW="80">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="QUERYSTORE" FIELD="QUERYSTORE_TYPE"
			NUM_ROW="90">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="GUESS_SCHEMA" FIELD="GUESS_SCHEMA" NUM_ROW="90">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="QUERY" FIELD="MEMO_SQL" NUM_ROW="100"
			REQUIRED="true">
			<DEFAULT>"select id, name from employee"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="MAPPING" FIELD="MAPPING_TYPE" NUM_ROW="110" SHOW="false" >
      		<DEFAULT>oracle_id</DEFAULT>
    	</PARAMETER>

	</PARAMETERS>
	<ADVANCED_PARAMETERS>

		<PARAMETER
			NAME="PROPERTIES"
			FIELD="TEXT"
			NUM_ROW="10"
			REPOSITORY_VALUE="PROPERTIES_STRING"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_WALLET')">
			<DEFAULT>""</DEFAULT>
		</PARAMETER>

		<PARAMETER
			NAME="NOTE"
			FIELD="LABEL"
			NUM_ROW="11"
			SHOW_IF="(USE_EXISTING_CONNECTION == 'false') and (CONNECTION_TYPE != 'ORACLE_WALLET')">
			<DEFAULT>*Note: Example for Additional JDBC Parameters: "parameterName1=value1&amp;&amp;parameterName2=value2"</DEFAULT>
		</PARAMETER>

		<PARAMETER NAME="IS_CONVERT_XMLTYPE" FIELD="CHECK" NUM_ROW="20">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CONVERT_XMLTYPE" FIELD="TABLE" NUM_ROW="21"
			NB_LINES="5" SHOW_IF="IS_CONVERT_XMLTYPE == 'true'">
			<ITEMS>
				<ITEM NAME="REFCOL" FIELD="COLUMN_LIST"/>
				<ITEM NAME="CONVETTYPE" FIELD="CLOSED_LIST">
					<ITEMS DEFAULT="STRING">
			            <ITEM NAME="STRING" VALUE="String"/>
					</ITEMS>
				</ITEM>
			</ITEMS>
		</PARAMETER>

		<PARAMETER NAME="ENCODING" FIELD="ENCODING_TYPE" NUM_ROW="100"
			REQUIRED="true" SHOW_IF="USE_EXISTING_CONNECTION == 'false'" SHOW="false">
			<DEFAULT>"ISO-8859-15"</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="USE_CURSOR" FIELD="CHECK" NUM_ROW="200">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="CURSOR_SIZE" FIELD="TEXT" NUM_ROW="200"
			SHOW_IF="(USE_CURSOR == 'true')">
			<DEFAULT>1000</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TRIM_ALL_COLUMN" FIELD="CHECK" NUM_ROW="300">
			<DEFAULT>false</DEFAULT>
		</PARAMETER>
		<PARAMETER NAME="TRIM_COLUMN" FIELD="TABLE" NUM_ROW="400"
			NB_LINES="5" SHOW_IF="TRIM_ALL_COLUMN == 'false'">
			<ITEMS BASED_ON_SCHEMA="true">
				<ITEM NAME="TRIM" FIELD="CHECK"></ITEM>
			</ITEMS>
		</PARAMETER>
	</ADVANCED_PARAMETERS>
	<CODEGENERATION>
     <IMPORTS>
       <IMPORT NAME="Driver-Oracle11g-1" MODULE="ojdbc6.jar" MVN="mvn:org.talend.libraries/ojdbc6/6.0.0"  REQUIRED_IF="(DB_VERSION == 'ORACLE_11') AND (USE_EXISTING_CONNECTION == 'false')" />
	   <IMPORT NAME="Driver-Oracle-XDB" MODULE="xdb.jar" MVN="mvn:org.talend.libraries/xdb/6.0.0"  REQUIRED_IF="SCHEMA.DB_TYPE IN ['XMLTYPE']" />
	   <IMPORT NAME="Driver-Oracle-XMLPARSERV2" BundleID="" MODULE="xmlparserv2.jar" MVN="mvn:org.talend.libraries/xmlparserv2/6.0.0"  REQUIRED_IF="SCHEMA.DB_TYPE IN ['XMLTYPE']" />
       <IMPORT NAME="Driver-Ojdbc6" MODULE="ojdbc6.jar" MVN="mvn:org.talend.libraries/ojdbc6/6.0.0"  REQUIRED_IF="(DB_VERSION == 'ORACLE_11-6') AND (USE_EXISTING_CONNECTION == 'false')" />
       <IMPORT NAME="Driver-Oracle12c" MODULE="ojdbc7.jar" MVN="mvn:org.talend.libraries/ojdbc7/6.0.0"  REQUIRED_IF="(DB_VERSION == 'ORACLE_12') AND (USE_EXISTING_CONNECTION == 'false')" />
     </IMPORTS>
	</CODEGENERATION>
	<RETURNS>
		<RETURN NAME="NB_LINE" TYPE="id_Integer" AVAILABILITY="AFTER" />
		<RETURN NAME="QUERY" TYPE="id_String" AVAILABILITY="FLOW" />
	</RETURNS>
</COMPONENT>

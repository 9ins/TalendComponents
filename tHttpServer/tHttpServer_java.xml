<?xml version="1.0" encoding="UTF-8"?>

<COMPONENT> 
  <HEADER 
       	AUTHOR="Kooin-Shin" 
  		PLATEFORM="ALL" 
       	SERIAL="" 
      	VERSION="0.90" 
       	STATUS="ALPHA" 
		COMPATIBILITY="ALL" 
 		RELEASE_DATE="20190104A" 
	    DATA_AUTO_PROPAGATE="false" 
    	SCHEMA_AUTO_PROPAGATE="true" 
    	HAS_CONDITIONAL_OUTPUTS="true" 
    	STARTABLE="true" 
		PARTITIONING="AUTO" 
  		LOG4J_ENABLED="true"   
  	>  
    <SIGNATURE/> 
  </HEADER>
  
  <FAMILIES> 
    <FAMILY>ChaosToCosmos</FAMILY> 
  </FAMILIES>
    
  <DOCUMENTATION> 
    <URL>http://www.chaostocosmos.com</URL> 
  </DOCUMENTATION>  
  
  <CONNECTORS> 
    <CONNECTOR NAME="HANDLER" CTYPE="COMPONENT_OK" MAX_INPUT="1" LINE_STYLE="2" COLOR="0000BB" />
    <CONNECTOR NAME="MONITOR" CTYPE="FLOW" MAX_OUTPUT="10" LINE_STYLE="2" COLOR="00BB00" />  
    <CONNECTOR NAME="ERROR" CTYPE="FLOW" MAX_OUTPUT="10" LINE_STYLE="2" COLOR="CC0000" />
    <CONNECTOR CTYPE="COMPONENT_OK" MAX_INPUT="1" MAX_OUTPUT="1" />  
    <CONNECTOR CTYPE="COMPONENT_ERROR" MAX_INPUT="1" MAX_OUTPUT="1" />
  </CONNECTORS>
     
  <PARAMETERS>
    <PARAMETER NAME="SCHEMA_MONITOR" FIELD="SCHEMA_TYPE" CONTEXT="MONITOR" REQUIRED="true" NUM_ROW="1" SHOW="true">
	  <TABLE>
		  <COLUMN NAME="START_DATE" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="LAST_TIME" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="TRANSACTION_PER_SEC" TYPE="id_String" CUSTOM="false" />	
		  <COLUMN NAME="TRANSACTION_TOTAL_COUNT" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="TRANSACTION_SUCCESS_COUNT" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="TRANSACTION_FAIL_COUNT" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="THREAD_ACTIVE_COUNT" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="THREAD_QUEUE_COUNT" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="TOTAL_TASK_COUNT" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="HEAP_USED" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="HEAP_INIT" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="HEAP_COMMITTED" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="HEAP_MAX" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="PHYSICAL_TOTAL" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="PHYSICAL_FREE" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="CPU_TIME" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="CPU_LOAD" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="SYSTEM_TOTAL_CPU_LOAD" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="DATE" TYPE="id_String" CUSTOM="false" />
	  </TABLE>
  	</PARAMETER>
    <PARAMETER NAME="SCHEMA_ERROR" FIELD="SCHEMA_TYPE" CONTEXT="ERROR" REQUIRED="true" NUM_ROW="1" SHOW="true">
	  <TABLE>
		  <COLUMN NAME="REMOTE_ADDRESS" TYPE="id_String" CUSTOM="false" />	
		  <COLUMN NAME="PROTOCOL" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="METHOD" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="RAW_PATH" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="RAW_QUERY" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="RES_CODE" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="RES_MSG" TYPE="id_String" CUSTOM="false" />	
		  <COLUMN NAME="ERROR_TRACE" TYPE="id_String" CUSTOM="false" />
		  <COLUMN NAME="DATE" TYPE="id_String" CUSTOM="false" /> 
	  </TABLE>
  	</PARAMETER>
	<PARAMETER NAME="SERVER_IP" FIELD="TEXT" NUM_ROW="10" GROUP="SERVER" SHOW="true">
		<DEFAULT>"localhost"</DEFAULT>
	</PARAMETER> 
	<PARAMETER NAME="SERVER_PORT" FIELD="TEXT" NUM_ROW="10" GROUP="SERVER" SHOW="true">
		<DEFAULT>9393</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="SERVER_BACKLOG" FIELD="TEXT" NUM_ROW="10" GROUP="SERVER" SHOW="true">
		<DEFAULT>0</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="DELAY_OF_START" FIELD="TEST" NUM_ROW="10" GROUP="SERVER" SHOW="true">
		<DEFAULT>0</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="IO_BUFFER" FIELD="OPENED_LIST" GROUP="SERVER" NUM_ROW="10" SHOW="true">
		<ITEMS DEFAULT="512K">
			<ITEM NAME="16K" VALUE="16"/>
			<ITEM NAME="32K" VALUE="32"/>
			<ITEM NAME="64K" VALUE="64"/>
			<ITEM NAME="128K" VALUE="128"/>
			<ITEM NAME="512K" VALUE="512"/>
			<ITEM NAME="1M" VALUE="1024"/> 
			<ITEM NAME="2M" VALUE="2048"/> 
			<ITEM NAME="3M" VALUE="3072"/>
			<ITEM NAME="5M" VALUE="5120"/>
			<ITEM NAME="10M" VALUE="10240"/>
			<ITEM NAME="20M" VALUE="20480"/>
			<ITEM NAME="50M" VALUE="51200"/>
			<ITEM NAME="100M" VALUE="102400"/> 
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="USE_WEB_HOME" FIELD="CHECK" NUM_ROW="11" GROUP="SERVER" SHOW="true">
		<DEFAULT>false</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="USE_WELCOME_FILE" FIELD="CHECK" NUM_ROW="11" GROUP="SERVER" SHOW_IF="USE_WEB_HOME == 'true'">
		<DEFAULT IF="USE_WEB_HOME == 'false'">false</DEFAULT>
	</PARAMETER>
    <PARAMETER NAME="WEB_HOME_PATH" FIELD="DIRECTORY" NUM_ROW="12" GROUP="SERVER" SHOW_IF="USE_WEB_HOME == 'true'">
		<DEFAULT>"."</DEFAULT>
	</PARAMETER> 
    <PARAMETER NAME="WELCOME_FILE" FIELD="FILENAME" NUM_ROW="12" GROUP="SERVER" SHOW_IF="(USE_WEB_HOME == 'true') AND (USE_WELCOME_FILE == 'true')">
		<DEFAULT IF="USE_WELCOME_FILE == 'false'">"welcome_en.html"</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="THREADPOOL_CORE_SIZE" FIELD="TEXT" NUM_ROW="30" GROUP="THREADPOOL" SHOW="true">
		<DEFAULT>10</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="THREADPOOL_MAX_SIZE" FIELD="TEXT" NUM_ROW="30" GROUP="THREADPOOL" SHOW="true">
		<DEFAULT>50</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="THREADPOOL_IDLE_DELAY" FIELD="TEXT" NUM_ROW="30" GROUP="THREADPOOL" SHOW="true">
		<DEFAULT>10000</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="THREADPOOL_WAIT_QUEUE_SIZE" FIELD="TEXT" NUM_ROW="30" GROUP="THREADPOOL" SHOW="true">
		<DEFAULT>100</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="MONITOR_PROBE_INTERVAL" FIELD="TEST" NUM_ROW="50" GROUP="MONITOR" SHOW="true">
		<DEFAULT>60</DEFAULT>
	</PARAMETER>  		 
	<PARAMETER NAME="INFO_MESSAGE_SIZE" FIELD="TEXT" NUM_ROW="50" GROUP="MONITOR" SHOW="true">
		<DEFAULT>3000</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="ERROR_CHAR_LIMIT" FIELD="TEXT" NUM_ROW="50" GROUP="MONITOR" SHOW="true">
		<DEFAULT>1000</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="ADD_UNIT" FIELD="CHECK" NUM_ROW="51" GROUP="MONITOR" SHOW="true">
		<DEFAULT>false</DEFAULT> 
	</PARAMETER>
  	<PARAMETER NAME="TIME_UNIT" FIELD="CLOSED_LIST"  GROUP="MONITOR" NUM_ROW="52" SHOW="true">
		<ITEMS DEFAULT="MS">
			<ITEM NAME="MS" VALUE="ms"/> 
			<ITEM NAME="SEC" VALUE="sec"/>
			<ITEM NAME="MIN" VALUE="min"/>
			<ITEM NAME="HR" VALUE="hr"/>
			<ITEM NAME="DY" VALUE="dy"/>
			<ITEM NAME="WK" VALUE="wk"/>
			<ITEM NAME="MO" VALUE="mo"/>
			<ITEM NAME="YR" VALUE="yr"/> 
		</ITEMS>
	</PARAMETER>
  	<PARAMETER NAME="SIZE_UNIT" FIELD="CLOSED_LIST" GROUP="MONITOR" NUM_ROW="52" SHOW="true">
		<ITEMS DEFAULT="MB">
			<ITEM NAME="B" VALUE="B"/>
			<ITEM NAME="KB" VALUE="KB"/>
			<ITEM NAME="MB" VALUE="MB"/>
			<ITEM NAME="GB" VALUE="GB"/>
			<ITEM NAME="TB" VALUE="TB"/>
			<ITEM NAME="PB" VALUE="PB"/>
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="QUANTITY_UNIT" FIELD="CLOSED_LIST" GROUP="MONITOR" NUM_ROW="52" SHOW="true">
		<ITEMS DEFAULT="QTY">
			<ITEM NAME="QTY" VALUE="qty"/>
			<ITEM NAME="NO" VALUE="no"/>
		</ITEMS>
	</PARAMETER>
	<PARAMETER NAME="PERCENT_UNIT" FIELD="CLOSED_LIST" GROUP="MONITOR" NUM_ROW="52" SHOW="true">
		<ITEMS DEFAULT="%">
			<ITEM NAME="%" VALUE="%"/> 
			<ITEM NAME="PC" VALUE="pc"/>
			<ITEM NAME="PERCENT" VALUE="percent"/>
		</ITEMS> 
	</PARAMETER>
  </PARAMETERS>
  
  <ADVANCED_PARAMETERS> 
  </ADVANCED_PARAMETERS>
    
  <CODEGENERATION> 
    <IMPORTS> 
      <IMPORT MODULE="chaos.http.0.9b.jar" NAME="chaos.http.0.9b" REQUIRED="true"/>  
      <IMPORT MODULE="commons-fileupload-1.4.jar" NAME="commons-fileupload-1.4" REQUIRED="true"/>  
      <IMPORT MODULE="commons-io-2.6.jar" NAME="commons-io-2.6" REQUIRED="true"/>  
      <IMPORT MODULE="servlet.jar" NAME="servlet" REQUIRED="true"/> 
      <IMPORT MODULE="image4j-0.7.2.jar" NAME="image4j-0.7.2" REQUIRED="true"/>
    </IMPORTS> 
  </CODEGENERATION>  
  
  <RETURNS> 
    <RETURN AVAILABILITY="FLOW" NAME="NB_TOTAL" TYPE="id_Long"/> 
    <RETURN AVAILABILITY="FLOW" NAME="NB_SUCCESS" TYPE="id_Long"/> 
    <RETURN AVAILABILITY="FLOW" NAME="NB_FAIL" TYPE="id_Long"/>
  </RETURNS> 
</COMPONENT>

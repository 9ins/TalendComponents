<COMPONENT>

  <HEADER
    PLATEFORM="ALL"
    SERIAL=""
    VERSION="0.102"
    STATUS="ALPHA"

    COMPATIBILITY="ALL"
    AUTHOR="Talend"
    RELEASE_DATE="20050320A"
    STARTABLE="true"
    LOG4J_ENABLED="true"
  >
    <SIGNATURE/>
  </HEADER>

  <FAMILIES>
    <FAMILY>Databases/Oracle</FAMILY>
    <FAMILY>ELT/Connections</FAMILY>
  </FAMILIES>

  <DOCUMENTATION>
    <URL/>
  </DOCUMENTATION>

  <CONNECTORS>
    <CONNECTOR CTYPE="FLOW" MAX_INPUT="0" MAX_OUTPUT="0"/>
    <CONNECTOR CTYPE="ITERATE" MAX_OUTPUT="0" MAX_INPUT="1"/>
    <CONNECTOR CTYPE="SUBJOB_OK" MAX_INPUT="1" />
    <CONNECTOR CTYPE="SUBJOB_ERROR" MAX_INPUT="1" />
    <CONNECTOR CTYPE="COMPONENT_OK"/>
    <CONNECTOR CTYPE="COMPONENT_ERROR"/>
    <CONNECTOR CTYPE="RUN_IF"/>
  </CONNECTORS>

    <PARAMETERS>

    <PARAMETER
      NAME="PROPERTY"
      FIELD="PROPERTY_TYPE"
      SHOW="true"
      NUM_ROW="10"
      REPOSITORY_VALUE="DATABASE:ORACLE"
	  NOT_SHOW_IF="(USE_TNS_FILE == 'true') AND (CONNECTION_TYPE == 'ORACLE_RAC')"
    />
    <PARAMETER
      NAME="CONNECTION_TYPE"
      FIELD="CLOSED_LIST"
      NUM_ROW="15"
      REPOSITORY_VALUE="CONNECTION_TYPE"
    >
      <ITEMS DEFAULT="ORACLE_SID">
        <ITEM NAME="ORACLE_SID" VALUE="ORACLE_SID" />
        <ITEM NAME="ORACLE_SERVICE_NAME" VALUE="ORACLE_SERVICE_NAME"/>
        <ITEM NAME="ORACLE_OCI" VALUE="ORACLE_OCI"/>
		<ITEM NAME="ORACLE_RAC" VALUE="ORACLE_RAC"/>
		<ITEM NAME="ORACLE_WALLET" VALUE="ORACLE_WALLET"/>
      </ITEMS>
    </PARAMETER>
	<PARAMETER NAME="DB_VERSION" FIELD="CLOSED_LIST" NUM_ROW="20"
		REPOSITORY_VALUE="DB_VERSION">
		<ITEMS DEFAULT="ORACLE_11-6">
			<ITEM NAME="ORACLE_12" VALUE="ORACLE_12" />
			<ITEM NAME="ORACLE_11-6" VALUE="ORACLE_11-6" />
			<ITEM NAME="ORACLE_11" VALUE="ORACLE_11" />
			<ITEM NAME="ORACLE_10" VALUE="ORACLE_10" />
			<ITEM NAME="ORACLE_9" VALUE="ORACLE_9" />
			<ITEM NAME="ORACLE_8" VALUE="ORACLE_8" />
		</ITEMS>
	</PARAMETER>
	<PARAMETER
		NAME="RAC_URL"
		FIELD="TEXT"
		NUM_ROW="21"
		REPOSITORY_VALUE="RAC_URL"
		REQUIRED="true"
		SHOW_IF="CONNECTION_TYPE == 'ORACLE_RAC'"
		>
		<DEFAULT>""</DEFAULT>
	</PARAMETER>
	<PARAMETER NAME="USE_TNS_FILE" FIELD="CHECK"
		NUM_ROW="22"
		NOT_SHOW_IF="CONNECTION_TYPE == 'ORACLE_RAC'"
		>
		<DEFAULT>false</DEFAULT>
	</PARAMETER>

    <PARAMETER
      NAME="TNS_FILE"
      FIELD="FILE"
      NUM_ROW="23"
      SHOW_IF="(USE_TNS_FILE == 'true') AND (CONNECTION_TYPE != 'ORACLE_RAC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="TNS_EDITOR"
	  FIELD="TNS_EDITOR"
      NUM_ROW="23"
	  SHOW_IF="(USE_TNS_FILE == 'true') AND (CONNECTION_TYPE != 'ORACLE_RAC')"
    >
    </PARAMETER>


    <PARAMETER
      NAME="HOST"
      FIELD="TEXT"
      NUM_ROW="25"
      REPOSITORY_VALUE="SERVER_NAME"
      NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_OCI') OR (CONNECTION_TYPE == 'ORACLE_WALLET') OR (CONNECTION_TYPE == 'ORACLE_RAC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="TYPE"
      FIELD="TEXT"
      NUM_ROW="25"
      SHOW="false"
      REPOSITORY_VALUE="TYPE"
    >
      <DEFAULT>Oracle</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="PORT"
      FIELD="TEXT"
      NUM_ROW="25"
      REPOSITORY_VALUE="PORT"
      NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_OCI') OR (CONNECTION_TYPE == 'ORACLE_WALLET') OR (CONNECTION_TYPE == 'ORACLE_RAC')"
    >
      <DEFAULT>"1521"</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="DBNAME"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_OCI') OR (CONNECTION_TYPE == 'ORACLE_WALLET') OR (CONNECTION_TYPE == 'ORACLE_RAC')"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="LOCAL_SERVICE_NAME"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SID"
      REQUIRED="true"
      SHOW_IF="CONNECTION_TYPE == 'ORACLE_OCI'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="SCHEMA_DB"
      FIELD="TEXT"
      NUM_ROW="30"
      REPOSITORY_VALUE="SCHEMA"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="USER"
      FIELD="TEXT"
      NUM_ROW="40"
      REPOSITORY_VALUE="USERNAME"
      REQUIRED="true"
	  NOT_SHOW_IF="CONNECTION_TYPE == 'ORACLE_WALLET'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="PASS"
      FIELD="PASSWORD"
      NUM_ROW="40"
      REPOSITORY_VALUE="PASSWORD"
      REQUIRED="true"
	  NOT_SHOW_IF="CONNECTION_TYPE == 'ORACLE_WALLET'"
    >
      <DEFAULT>""</DEFAULT>
    </PARAMETER>

    <PARAMETER
      NAME="ENCODING"
      FIELD="ENCODING_TYPE"
      NUM_ROW="45"
      REQUIRED="true"
      SHOW="false"
    >
      <DEFAULT>"ISO-8859-15"</DEFAULT>
    </PARAMETER>

	<PARAMETER
		NAME="PROPERTIES"
		FIELD="TEXT"
		NUM_ROW="50"
		REPOSITORY_VALUE="PROPERTIES_STRING"
		NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_WALLET')">
		<DEFAULT>""</DEFAULT>
	</PARAMETER>

	<PARAMETER
		NAME="NOTE"
		FIELD="LABEL"
		NUM_ROW="51"
		NOT_SHOW_IF="(CONNECTION_TYPE == 'ORACLE_WALLET') OR (CONNECTION_TYPE == 'ORACLE_RAC')">
			<DEFAULT>*Note: Example for Additional JDBC Parameters: "parameterName1=value1&amp;&amp;parameterName2=value2"</DEFAULT>

	</PARAMETER>

	<PARAMETER
      NAME="USE_SHARED_CONNECTION"
	  FIELD="CHECK"
      NUM_ROW="55"
      REQUIRED="true"
	  SHOW_IF="SPECIFY_DATASOURCE_ALIAS=='false'"
    >
	<DEFAULT>false</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="SHARED_CONNECTION_NAME"
	  FIELD="TEXT"
      NUM_ROW="55"
      REQUIRED="true"
	  SHOW_IF="(USE_SHARED_CONNECTION == 'true' AND SPECIFY_DATASOURCE_ALIAS=='false')"
    >
	<DEFAULT></DEFAULT>
    </PARAMETER>

	<PARAMETER
	  NAME="NOTE"
      FIELD="LABEL"
      NUM_ROW="63"
	  GROUP="DATASOURCE"
	  SHOW_IF="USE_SHARED_CONNECTION=='false'"
    >
    	<DEFAULT>This option only applies when deploying and running in the Talend Runtime</DEFAULT>
    </PARAMETER>
	<PARAMETER
      NAME="SPECIFY_DATASOURCE_ALIAS"
	  FIELD="CHECK"
      NUM_ROW="64"
      REQUIRED="true"
	  GROUP="DATASOURCE"
	  SHOW_IF="USE_SHARED_CONNECTION=='false'"
    >
    </PARAMETER>
	<PARAMETER
      NAME="DATASOURCE_ALIAS"
	  FIELD="TEXT"
      NUM_ROW="65"
      REQUIRED="true"
	  GROUP="DATASOURCE"
	  SHOW_IF="(SPECIFY_DATASOURCE_ALIAS=='true' AND USE_SHARED_CONNECTION=='false')"
    >
	<DEFAULT>""</DEFAULT>
    </PARAMETER>

	<PARAMETER
      NAME="JDBC_URL"
	  FIELD="TEXT"
      NUM_ROW="35"
      REQUIRED="true"
	  SHOW_IF="CONNECTION_TYPE == 'ORACLE_WALLET'"
    >
	<DEFAULT>"jdbc:oracle:thin:USER/MDP@server"</DEFAULT>
    </PARAMETER>
   </PARAMETERS>

	<ADVANCED_PARAMETERS>
		<PARAMETER NAME="AUTO_COMMIT" FIELD="CHECK" NUM_ROW="10">
			<DEFAULT>false</DEFAULT>
    	</PARAMETER>
	</ADVANCED_PARAMETERS>

   <CODEGENERATION>
    <IMPORTS>
       <IMPORT NAME="TNS" MODULE="tns.jar" MVN="mvn:org.talend.libraries/tns/6.0.0"  REQUIRED="true" />
       <IMPORT NAME="Talend-oracle-timestamptz" MODULE="talend-oracle-timestamptz.jar" MVN="mvn:org.talend.libraries/talend-oracle-timestamptz/6.0.0"  UrlPath="platform:/plugin/org.talend.libraries.custom/lib/talend-oracle-timestamptz.jar" REQUIRED_IF="(DB_VERSION == 'ORACLE_11-6')  OR (DB_VERSION=='ORACLE_11') OR (DB_VERSION=='ORACLE_10')" />
       <IMPORT NAME="ORACLE_12"  MODULE="ojdbc7.jar" MVN="mvn:org.talend.libraries/ojdbc7/6.0.0"  BundleID="" REQUIRED_IF="(DB_VERSION == 'ORACLE_12')" />
       <IMPORT NAME="ORACLE_11-6"  MODULE="ojdbc6.jar" MVN="mvn:org.talend.libraries/ojdbc6/6.0.0"  BundleID="" REQUIRED_IF="(DB_VERSION == 'ORACLE_11-6')" />
       <IMPORT NAME="ORACLE_11"  MODULE="ojdbc6.jar" MVN="mvn:org.talend.libraries/ojdbc6/6.0.0"  BundleID="" REQUIRED_IF="(DB_VERSION == 'ORACLE_11') " />
       <IMPORT NAME="ORACLE_10"  MODULE="ojdbc14.jar" MVN="mvn:org.talend.libraries/ojdbc14/6.0.0"  BundleID="" REQUIRED_IF="(DB_VERSION == 'ORACLE_10') " />
       <IMPORT NAME="ORACLE_9"  MODULE="ojdbc14-9i.jar" MVN="mvn:org.talend.libraries/ojdbc14-9i/6.0.0"  BundleID="" REQUIRED_IF="(DB_VERSION == 'ORACLE_9') " />
       <IMPORT NAME="ORACLE_8"  MODULE="ojdbc12.jar" MVN="mvn:org.talend.libraries/ojdbc12/6.0.0"  BundleID="" REQUIRED_IF="(DB_VERSION == 'ORACLE_8') " />
    </IMPORTS>
   </CODEGENERATION>

   <RETURNS/>
</COMPONENT>
